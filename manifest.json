{
  "manifest_version": 3,
  "name": "Owl Bookmarks",
  "version": "1.0.0",
  "description": "A beautiful bookmark manager with owl mascot and sync capabilities",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "bookmarks"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}